# NeuroResolv Backend Configuration
# Copy this file to .env and fill in your values

# Database
DATABASE_URL=sqlite+aiosqlite:///./neuroresolv.db
# For PostgreSQL: DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/neuroresolv

# JWT Authentication
SECRET_KEY=your-super-secret-key-change-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Google Gemini API (via ADK)
GOOGLE_API_KEY=your-google-api-key-here

# Opik Configuration
OPIK_API_KEY=your-opik-api-key-here
OPIK_WORKSPACE=default
OPIK_PROJECT_NAME=neuroresolv

# ChromaDB
CHROMA_PERSIST_DIRECTORY=./chroma_data

# Environment
ENVIRONMENT=development
DEBUG=true
